Section "ServerLayout"
    Identifier     "Layout0"
    Option         "AutoAddDevices" "false"
EndSection

Section "Device"
    Identifier     "Device1"
    Driver         "nvidia"
    VendorName     "NVIDIA Corporation"
    BusID          ""
    Option         "UseEDID" "false"
    Option          "ConnectedMonitor" "DFP,CRT"
EndSection

Section "Extensions"
    Option "Composite" "Enable"
EndSection

Section "Files"
    ModulePath ",/usr/lib/xorg/modules"
EndSection
